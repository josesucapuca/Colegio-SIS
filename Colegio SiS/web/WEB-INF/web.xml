<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CMenu</servlet-name>
        <servlet-class>pe.edu.sis.application.web.controller.CMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CPlanCurricular</servlet-name>
        <servlet-class>pe.edu.sis.application.web.controller.CPlanCurricular</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CMenu</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CPlanCurricular</servlet-name>
        <url-pattern>/plancurricular</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            1000
        </session-timeout>
    </session-config>
</web-app>
